{
  "name": "@vtex/address-form",
  "version": "3.24.9",
  "description": "address-form React component",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "babel --delete-dir-on-start --config-file ./.babelrc . -d lib -x .js,.jsx,.ts,.tsx && yarn add-locales",
    "add-locales": "cp -R -f ../messages/ lib/locales/",
    "test": "jest --env=jsdom",
    "test:coverage": "jest --env=jsdom --coverage",
    "test:watch": "jest --env=jsdom --watch",
    "prepublishOnly": "yarn build"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "15.x || 16.x",
    "react-intl": "^2.7.2 || 3.x || 4.x || 5.x",
    "vtex-tachyons": "^2.5.0"
  },
  "dependencies": {
    "@vtex/styleguide": "^9.112.28",
    "axios": "^0.16.2",
    "classnames": "^2.2.5",
    "load-google-maps-api": "^1.0.0",
    "lodash": "^4.17.4",
    "msk": "^1.0.5",
    "react-intl": "^2.8.0",
    "recompose": "^0.27.1",
    "splunk-events": "^1.7.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.15.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@types/jest": "^26.0.19",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vtex/tsconfig": "^0.5.6",
    "babel-jest": "^26.3.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.8.0",
    "enzyme-to-json": "^3.3.4",
    "i18n-iso-countries": "^1.13.0",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jest-enzyme": "^7.1.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-test-renderer": "^16.7.0",
    "typescript": "3.9.7"
  },
  "resolutions": {
    "jest-environment-jsdom": "^26.3.0"
  },
  "author": "Breno Calazans (@klzns)",
  "homepage": "https://github.com/vtex/address-form",
  "license": "AGPL-3.0-only",
  "repository": "https://github.com/vtex/address-form",
  "keywords": [
    "react-component",
    "vtex",
    "checkout",
    "address"
  ],
  "jest": {
    "testURL": "http://localhost/",
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/test-modules"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/es/",
      "<rootDir>/lib/",
      "<rootDir>/umd/"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]lib[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]umd[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]es[/\\\\].+\\.(js|jsx)$",
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "\\.(css|jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)(\\?.*)?$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.(j|t)sx?$": "<rootDir>/jest.transform.js"
    }
  }
}
